// A helper chip for the project
// File name: projects/5/Or16Way.hdl
/**
 * 16-way Or gate:
 * out = in[0] Or in[1] Or ... Or in[15]
 */
CHIP Or16Way {
    IN in[16];
    OUT out;

    PARTS:
    Or8Way(in=in[0..7], out=part1);
    Or8Way(in=in[8..15], out=part2);
    Or(a=part1,b=part2, out=out);
}
